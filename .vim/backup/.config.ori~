Host shell
    Hostname shellmix.com
    Port 22
    User logout
    Protocol 2
    IdentityFile ~/.ssh/shell
    Compression yes
ProxyCommand connect -4 -S localhost:9050 $(tor-resolve %h localhost:9050) %p
    ServerAliveInterval 60


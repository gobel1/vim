#!/usr/bin/php
<?php //test.php

for($i=1;$i<$argc;$i++) {
    $v = split('::',$argv[$i]);
    $a = split('=',$v[1]);
    if($v[0] == 'get' || $v[0] == 'GET') {
        $_GET[$a[0]] = $a[1];
    } elseif($v[0] == 'post' || $v[0] == 'POST') {
        $_POST[$a[0]] = $a[1];
    } else {
        unset($v,$a);
    }
}
printf("REQ=%s a=%s b=%s\n\r",$v[0],$a[0],$a[1]);
include('index.php');

?>


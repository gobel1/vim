## Ori
# Prevent "Too many authentication faillure" when ssh agent try each key in .ssh directory
IdentitiesOnly yes 

Host *.univmed.fr
    Port 22
    User root
    Protocol 2
    IdentityFile ~/.ssh/stjerome

Host sph
    Hostname sphynx.pr.univmed.fr
    Port 22
    User root
    Protocol 2
    IdentityFile ~/.ssh/stjerome
    ProxyCommand ssh ecume exec nc %h %p

